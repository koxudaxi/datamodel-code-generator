{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Person",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "null_name": {
      "type": ["string", "null"]
    },
    "age": {
      "type": "integer"
    },
    "null_age": {
      "type": ["integer", "null"]
    }
  },
  "required": ["name", "null_age"]
}
