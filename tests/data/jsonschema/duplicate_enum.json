{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "User",
  "type": "object",
  "properties": {
      "name": {
          "type": "string"
      },
      "animal": {
        "type": "string",
        "enum": [
            "dog",
            "cat",
            "snake"
        ]
    },
      "pet": {
        "type": "string",
        "enum": [
            "dog",
            "cat",
            "snake"
        ]
    }
  }
}