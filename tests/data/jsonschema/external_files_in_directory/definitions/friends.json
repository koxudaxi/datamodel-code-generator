{
  "$id": "friends.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Friends",
  "type": "array",
  "items": {
    "properties": {
      "name": {
        "type": "string",
        "example": "John Doe"
      },
      "phone_number": {
        "type": "string",
        "example": "(555) 555-1234"
      }
    },
    "additionalProperties": true,
    "required": [
      "name"
    ]
  }
}
