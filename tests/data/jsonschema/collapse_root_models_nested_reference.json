{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "refs": {
      "$ref": "#/$defs/SomeRefs"
    }
  },
  "required": [
    "refs"
  ],
  "$defs": {
    "SomeRefs": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/SomeRef"
      }
    },
    "SomeRef": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        }
      },
      "required": [
        "id"
      ]
    }
  }
}
