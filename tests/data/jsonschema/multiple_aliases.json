{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Root",
  "type": "object",
  "properties": {
    "my_field": {
      "type": "string"
    },
    "other_field": {
      "type": "integer"
    },
    "user": {
      "title": "User",
      "type": "object",
      "properties": {
        "user_name": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "merged_obj": {
      "allOf": [
        {
          "type": "object",
          "properties": {
            "base_field": {
              "type": "string"
            }
          },
          "required": ["inherited_required"]
        },
        {
          "type": "object",
          "properties": {
            "extra_field": {
              "type": "integer"
            }
          }
        }
      ]
    },
    "any_value_field": true
  }
}
