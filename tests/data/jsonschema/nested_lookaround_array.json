{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "LookaroundStringArray": {
      "description": "Array of strings with lookaround pattern applied to items.",
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^(?=.*[A-Z])(?=.*[0-9]).+$",
        "minLength": 1
      }
    }
  },
  "type": "object",
  "properties": {
    "codes": { "$ref": "#/definitions/LookaroundStringArray" }
  }
}
