{%- if description %}
    """
    {{ description | indent(4) }}
    """
{%- endif %}
    {{ class_name }} = TypedDict('{{ class_name }}', {
{%- for field in fields %}
    '{{ field.name }}': {{ field.type_hint }},
    {%- if field.docstring %}
    """
    {{ field.docstring | indent(4) }}
    """
    {%- endif %}
{%- endfor -%}
})

